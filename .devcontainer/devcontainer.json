  {
  "name": "User Story Mapper Dev Container",
  "build": { "dockerfile": "../Dockerfile" },
  "features": {},
  "customizations": {
    "vscode": {
      "settings": {},
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-python.requests",
        "continer-tools.vscode-docker"
      ]
    }
  },
  "workspaceMount": "source=${localWorkspaceFolder},target=/app,type=bind,consistency=cached",
  "workspaceFolder": "/app",
  "postCreateCommand": "python -m src.main"
}
